{
  "name": "simple-react-grid",
  "version": "1.0.0",
  "description": "Grid System for React Js",
  "main": "build/index.js",
  "scripts": {
    "build": "babel lib -d build",
    "lint": "eslint lib/**; exit 0",
    "lint:watch": "esw -w lib/**",
    "prepublish": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rabingaire/simple-react-grid.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rabingaire/simple-react-grid/issues"
  },
  "homepage": "https://github.com/rabingaire/simple-react-grid#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.8.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.2",
    "jest": "^21.2.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "styled-components": "^2.2.1"
  },
  "jest": {
    "testMatch": [
      "**/*.spec.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
